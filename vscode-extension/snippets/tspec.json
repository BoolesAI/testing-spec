{
  "TSpec HTTP Basic Template": {
    "prefix": "tspec-http",
    "body": [
      "version: \"1.0\"",
      "description: \"${1:Test description}\"",
      "",
      "metadata:",
      "  prompt: |",
      "    ${2:Describe what this test verifies}",
      "  related_code:",
      "    - \"${3:src/path/to/code.js[1-10]}\"",
      "  test_category: \"${4|functional,integration,performance,security|}\"",
      "  risk_level: \"${5|low,medium,high,critical|}\"",
      "  tags: [\"${6:tag1}\", \"${7:tag2}\"]",
      "  priority: \"${8|low,medium,high|}\"",
      "  timeout: \"${9:10s}\"",
      "",
      "http:",
      "  method: \"${10|GET,POST,PUT,DELETE,PATCH|}\"",
      "  path: \"${11:/api/v1/endpoint}\"",
      "  headers:",
      "    Content-Type: \"application/json\"",
      "  body:",
      "    json:",
      "      ${12:key}: \"${13:value}\"",
      "",
      "assertions:",
      "  - type: \"json_path\"",
      "    expression: \"\\$.status\"",
      "    operator: \"equals\"",
      "    expected: ${14:200}",
      "  - type: \"json_path\"",
      "    expression: \"${15:\\$.body.data}\"",
      "    operator: \"${16|exists,equals,contains,matches|}\"",
      "$0"
    ],
    "description": "Create a basic HTTP TSpec test file"
  },
  "TSpec gRPC Basic Template": {
    "prefix": "tspec-grpc",
    "body": [
      "version: \"1.0\"",
      "description: \"${1:Test description}\"",
      "",
      "metadata:",
      "  prompt: |",
      "    ${2:Describe what this test verifies}",
      "  related_code:",
      "    - \"${3:src/path/to/code.js[1-10]}\"",
      "  test_category: \"${4|functional,integration,performance,security|}\"",
      "  risk_level: \"${5|low,medium,high,critical|}\"",
      "  tags: [\"${6:grpc}\"]",
      "  priority: \"${7|low,medium,high|}\"",
      "  timeout: \"${8:10s}\"",
      "",
      "grpc:",
      "  service: \"${9:ServiceName}\"",
      "  method: \"${10:MethodName}\"",
      "  message:",
      "    ${11:field}: \"${12:value}\"",
      "",
      "assertions:",
      "  - type: \"json_path\"",
      "    expression: \"\\$.grpcCode\"",
      "    operator: \"equals\"",
      "    expected: \"${13:OK}\"",
      "$0"
    ],
    "description": "Create a basic gRPC TSpec test file"
  },
  "TSpec GraphQL Basic Template": {
    "prefix": "tspec-graphql",
    "body": [
      "version: \"1.0\"",
      "description: \"${1:Test description}\"",
      "",
      "metadata:",
      "  prompt: |",
      "    ${2:Describe what this test verifies}",
      "  related_code:",
      "    - \"${3:src/path/to/code.js[1-10]}\"",
      "  test_category: \"${4|functional,integration,performance,security|}\"",
      "  risk_level: \"${5|low,medium,high,critical|}\"",
      "  tags: [\"${6:graphql}\"]",
      "  priority: \"${7|low,medium,high|}\"",
      "  timeout: \"${8:10s}\"",
      "",
      "graphql:",
      "  query: |",
      "    ${9:query {",
      "      users {",
      "        id",
      "        name",
      "      }",
      "    }}",
      "  variables:",
      "    ${10:id}: \"${11:value}\"",
      "",
      "assertions:",
      "  - type: \"json_path\"",
      "    expression: \"\\$.status\"",
      "    operator: \"equals\"",
      "    expected: 200",
      "  - type: \"json_path\"",
      "    expression: \"\\$.body.data\"",
      "    operator: \"exists\"",
      "$0"
    ],
    "description": "Create a basic GraphQL TSpec test file"
  },
  "TSpec WebSocket Basic Template": {
    "prefix": "tspec-websocket",
    "body": [
      "version: \"1.0\"",
      "description: \"${1:Test description}\"",
      "",
      "metadata:",
      "  prompt: |",
      "    ${2:Describe what this test verifies}",
      "  related_code:",
      "    - \"${3:src/path/to/code.js[1-10]}\"",
      "  test_category: \"${4|functional,integration,performance,security|}\"",
      "  risk_level: \"${5|low,medium,high,critical|}\"",
      "  tags: [\"${6:websocket}\"]",
      "  priority: \"${7|low,medium,high|}\"",
      "  timeout: \"${8:30s}\"",
      "",
      "websocket:",
      "  url: \"${9:wss://example.com/ws}\"",
      "  messages:",
      "    - ${10:message}",
      "",
      "assertions:",
      "  - type: \"json_path\"",
      "    expression: \"\\$.status\"",
      "    operator: \"equals\"",
      "    expected: 101",
      "$0"
    ],
    "description": "Create a basic WebSocket TSpec test file"
  },
  "Metadata Block": {
    "prefix": "sec-metadata",
    "body": [
      "metadata:",
      "  prompt: |",
      "    ${1:Describe what this test verifies}",
      "  related_code:",
      "    - \"${2:src/path/to/code.js}\"",
      "  test_category: \"${3|functional,integration,performance,security|}\"",
      "  risk_level: \"${4|low,medium,high,critical|}\"",
      "  tags: [\"${5:tag1}\"]",
      "  priority: \"${6|low,medium,high|}\"",
      "  timeout: \"${7:10s}\"$0"
    ],
    "description": "Insert metadata block"
  },
  "Environment Block": {
    "prefix": "sec-environment",
    "body": [
      "environment:",
      "  name: \"\\${ENV_NAME|${1:staging}}\"",
      "  host: \"\\${API_HOST|${2:api.example.com}}\"",
      "  scheme: \"${3|https,http|}\"$0"
    ],
    "description": "Insert environment configuration block"
  },
  "Variables Block": {
    "prefix": "sec-variables",
    "body": [
      "variables:",
      "  ${1:key}: \"${2:value}\"",
      "  request_id: \"\\${uuid}\"$0"
    ],
    "description": "Insert variables block"
  },
  "Assertions Block": {
    "prefix": "sec-assertions",
    "body": [
      "assertions:",
      "  - type: \"json_path\"",
      "    expression: \"\\$.status\"",
      "    operator: \"equals\"",
      "    expected: ${1:200}",
      "  - type: \"json_path\"",
      "    expression: \"${2:\\$.body.data}\"",
      "    operator: \"${3|exists,equals,contains,matches|}\"$0"
    ],
    "description": "Insert assertions block"
  },
  "HTTP Request Block": {
    "prefix": "http-request",
    "body": [
      "http:",
      "  method: \"${1|GET,POST,PUT,DELETE,PATCH|}\"",
      "  path: \"${2:/api/v1/endpoint}\"",
      "  headers:",
      "    Content-Type: \"${3:application/json}\"",
      "  body:",
      "    json:",
      "      ${4:key}: \"${5:value}\"$0"
    ],
    "description": "Insert HTTP request block"
  },
  "HTTP Request with Auth": {
    "prefix": "http-auth",
    "body": [
      "http:",
      "  method: \"${1|GET,POST,PUT,DELETE,PATCH|}\"",
      "  path: \"${2:/api/v1/endpoint}\"",
      "  headers:",
      "    Content-Type: \"application/json\"",
      "    Authorization: \"Bearer \\${${3:token}}\"$0"
    ],
    "description": "Insert HTTP request with Bearer token"
  },
  "Lifecycle Block": {
    "prefix": "sec-lifecycle",
    "body": [
      "lifecycle:",
      "  setup:",
      "    - action: \"${1|script,extract,output|}\"",
      "      scope: \"${2|test,assert,run,data|}\"",
      "      ${3:source}: |",
      "        ${4:return {};}",
      "  teardown:",
      "    - action: \"${5|script,extract,output|}\"",
      "      scope: \"${6|test,assert,run,data|}\"$0"
    ],
    "description": "Insert lifecycle hooks block with scope control"
  },
  "Lifecycle Script Action": {
    "prefix": "lifecycle-script",
    "body": [
      "- action: \"script\"",
      "  scope: \"${1|test,assert,run,data|}\"",
      "  source: |",
      "    ${2:return {};}$0"
    ],
    "description": "Insert script action in lifecycle"
  },
  "Lifecycle Extract Action": {
    "prefix": "lifecycle-extract",
    "body": [
      "- action: \"extract\"",
      "  scope: \"${1|assert,test|}\"",
      "  vars:",
      "    ${2:variable_name}: \"${3:\\$.data.field}\"$0"
    ],
    "description": "Insert extract action in lifecycle"
  },
  "Lifecycle Output Action": {
    "prefix": "lifecycle-output",
    "body": [
      "- action: \"output\"",
      "  scope: \"test\"",
      "  config:",
      "    save_response_on_failure: ${1:true}$0"
    ],
    "description": "Insert output action in lifecycle"
  },
  "Status Code Assertion": {
    "prefix": "assert-status",
    "body": [
      "- type: \"json_path\"",
      "  expression: \"\\$.status\"",
      "  operator: \"equals\"",
      "  expected: ${1:200}$0"
    ],
    "description": "Insert status code assertion (unified approach)"
  },
  "JSON Path Assertion": {
    "prefix": "assert-jsonpath",
    "body": [
      "- type: \"json_path\"",
      "  expression: \"${1:\\$.body.data}\"",
      "  operator: \"${2|exists,equals,contains,matches,not_empty|}\"",
      "  ${3:expected: \"${4:value}\"}$0"
    ],
    "description": "Insert JSON path assertion"
  },
  "JSON Path Equals Assertion": {
    "prefix": "assert-jsonpath-equals",
    "body": [
      "- type: \"json_path\"",
      "  expression: \"${1:\\$.body.data.field}\"",
      "  operator: \"equals\"",
      "  expected: ${2:value}$0"
    ],
    "description": "Insert JSON path equals assertion"
  },
  "JSON Path Matches Assertion": {
    "prefix": "assert-jsonpath-matches",
    "body": [
      "- type: \"json_path\"",
      "  expression: \"${1:\\$.body.data.field}\"",
      "  operator: \"matches\"",
      "  pattern: \"${2:^[A-Za-z0-9]+$}\"$0"
    ],
    "description": "Insert JSON path regex matches assertion"
  },
  "Header Assertion": {
    "prefix": "assert-header",
    "body": [
      "- type: \"json_path\"",
      "  expression: \"\\$.header['${1:Content-Type}']\"",
      "  operator: \"${2|equals,contains|}\"",
      "  expected: \"${3:application/json}\"$0"
    ],
    "description": "Insert header assertion (unified approach)"
  },
  "Response Time Assertion": {
    "prefix": "assert-response-time",
    "body": [
      "- type: \"response_time\"",
      "  max_ms: ${1:1000}$0"
    ],
    "description": "Insert response time assertion"
  },
  "JavaScript Assertion": {
    "prefix": "assert-javascript",
    "body": [
      "- type: \"javascript\"",
      "  source: |",
      "    ${1:return response.body.data !== null;}$0"
    ],
    "description": "Insert custom JavaScript assertion"
  },
  "String Assertion": {
    "prefix": "assert-string",
    "body": [
      "- type: \"string\"",
      "  expression: \"${1:\\$.body.data.field}\"",
      "  operator: \"${2|equals,contains,matches|}\"",
      "  expected: \"${3:value}\"$0"
    ],
    "description": "Insert string type assertion (coerces value to string)"
  },
  "Number Assertion": {
    "prefix": "assert-number",
    "body": [
      "- type: \"number\"",
      "  expression: \"${1:\\$.body.data.count}\"",
      "  operator: \"${2|equals,gt,gte,lt,lte|}\"",
      "  expected: ${3:0}$0"
    ],
    "description": "Insert number type assertion (coerces value to number)"
  },
  "Regex Assertion": {
    "prefix": "assert-regex",
    "body": [
      "- type: \"regex\"",
      "  expression: \"${1:\\$.body.data.url}\"",
      "  pattern: \"${2:/users/([a-f0-9-]+)}\"",
      "  extract_group: ${3:1}",
      "  operator: \"${4|exists,equals|}\"$0"
    ],
    "description": "Insert regex assertion with capture group"
  },
  "XML Path Assertion": {
    "prefix": "assert-xml",
    "body": [
      "- type: \"xml_path\"",
      "  expression: \"${1://root/element}\"",
      "  operator: \"${2|exists,equals,contains|}\"",
      "  expected: \"${3:value}\"$0"
    ],
    "description": "Insert XPath assertion for XML responses"
  },
  "File Exist Assertion": {
    "prefix": "assert-file-exist",
    "body": [
      "- type: \"file_exist\"",
      "  expression: \"${1:./output/report.html}\"",
      "  message: \"${2:File should exist}\"$0"
    ],
    "description": "Insert file exist assertion"
  },
  "File Read Assertion": {
    "prefix": "assert-file-read",
    "body": [
      "- type: \"file_read\"",
      "  expression: \"${1:./output/result.json}\"",
      "  operator: \"${2|not_empty,contains,equals,matches|}\"",
      "  ${3:expected}: \"${4:value}\"$0"
    ],
    "description": "Insert file read assertion"
  },
  "gRPC Code Assertion": {
    "prefix": "assert-grpc",
    "body": [
      "- type: \"json_path\"",
      "  expression: \"\\$.grpcCode\"",
      "  operator: \"equals\"",
      "  expected: \"${1:OK}\"$0"
    ],
    "description": "Insert gRPC code assertion (unified approach)"
  },
  "Variable - Environment": {
    "prefix": "var-env",
    "body": "\\${env.${1:VARIABLE_NAME}}$0",
    "description": "Insert environment variable reference"
  },
  "Variable - Extract": {
    "prefix": "var-extract",
    "body": "\\${extract.${1:variable_name}}$0",
    "description": "Insert extracted value reference"
  },
  "Variable - UUID": {
    "prefix": "var-uuid",
    "body": "\\${uuid}$0",
    "description": "Insert UUID generator function"
  },
  "Variable - Timestamp": {
    "prefix": "var-timestamp",
    "body": "\\${timestamp}$0",
    "description": "Insert timestamp function"
  },
  "Variable - Random Int": {
    "prefix": "var-random",
    "body": "\\${random_int(${1:1},${2:100})}$0",
    "description": "Insert random integer function"
  },
  "Variable - With Default": {
    "prefix": "var-default",
    "body": "\\${${1:VARIABLE}|${2:default_value}}$0",
    "description": "Insert variable with default value"
  }
}
